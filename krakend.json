{
  "version": 3,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend"
  },
  "endpoints": [
    {
      "endpoint": "/test/{id}",
      "backend": [
        {
          "host": [
            "http://localhost:8080"
          ],
          "url_pattern": "/__health"
        }
      ]
    }
  ],
  "extra_config": {
    "plugin/http-server": {
      "name": ["authentication"],
      "authentication": {
        "path": "/token"
      }
    }
  }
}